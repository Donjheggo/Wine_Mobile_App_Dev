{
  "expo": {
    "name": "vintage-associates",
    "slug": "vintage-associates",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Poppins-Light.ttf",
        "name": "Light"
      },
      {
        "asset": "./assets/fonts/Poppins-Regular.ttf",
        "name": "Regular"
      },
      {
        "asset": "./assets/fonts/Poppins-Medium.ttf",
        "name": "Medium"
      },
      {
        "asset": "./assets/fonts/Poppins-SemiBold.ttf",
        "name": "SemiBold"
      },
      {
        "asset": "./assets/fonts/Poppins-Bold.ttf",
        "name": "Bold"
      }
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Vintage Associates to access your camera",
          "microphonePermission": "Allow Vintage Associates to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
